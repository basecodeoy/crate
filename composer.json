{
    "name": "basecodeoy/crate",
    "description": "A package to supercharge your packages with Laravel.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "BaseCode Oy",
        "laravel",
        "crate"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/basecodeoy/crate",
    "support": {
        "issues": "https://github.com/basecodeoy/crate/issues",
        "source": "https://github.com/basecodeoy/crate",
        "security": "https://github.com/basecodeoy/crate/blob/main/.github/SECURITY.md"
    },
    "require": {
        "php": "^8.3",
        "basecodeoy/analyzer": "^1.0.0",
        "illuminate/contracts": "^11.33.2",
        "nesbot/carbon": "^2.72.5 || ^3.0.0",
        "spatie/laravel-package-tools": "^1.16.6"
    },
    "require-dev": {
        "basecodeoy/standards": "^1.1.0"
    },
    "autoload": {
        "psr-4": {
            "BaseCodeOy\\Crate\\": "src"
        },
        "files": [
            "src/TestBench/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "audit": {
            "abandoned": "report"
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
